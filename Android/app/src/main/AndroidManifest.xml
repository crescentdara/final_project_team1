<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <queries>
        <package android:name="com.skt.tmap.ku" />
        <package android:name="com.skt.skaf.l001mtm091" />
    </queries>

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:networkSecurityConfig="@xml/network_security_config"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Final_Project_Team1"
        android:usesCleartextTraffic="true">

        <!-- ✅ 여기! application 바로 아래에 한 번만 선언 -->
        <meta-data
            android:name="com.naver.maps.map.NCP_KEY_ID"
            android:value="eeh71qt74n" />

        <activity android:name=".MainActivity" />

        <activity android:name=".ui.surveyList.ArActivity" />
        <activity android:name=".ui.surveyList.FullMapActivity" />
        <activity android:name=".ui.surveyList.RouteActivity" />
        <activity android:name=".ui.surveyList.SurveyListActivity" />
        <activity
            android:name=".ui.login.LoginActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ui.category.CategoryActivity" />
        <activity android:name=".ui.survey.SurveyStatusActivity" />

    </application>
</manifest>
